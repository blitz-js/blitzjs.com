{
  "name": "blitzjs-com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "lint": "alex docs/**/*.mdx"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "trailingComma": "all"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn run lint"
    }
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.55",
    "@docusaurus/plugin-content-blog": "^2.0.0-alpha.55",
    "@docusaurus/plugin-content-docs": "^2.0.0-alpha.55",
    "@docusaurus/plugin-content-pages": "^2.0.0-alpha.55",
    "@docusaurus/plugin-google-analytics": "^2.0.0-alpha.55",
    "@docusaurus/plugin-google-gtag": "^2.0.0-alpha.55",
    "@docusaurus/plugin-sitemap": "^2.0.0-alpha.55",
    "@theme-ui/prism": "v0.4.0-rc.1",
    "algoliasearch": "^4.4.0",
    "algoliasearch-helper": "^3.2.2",
    "classnames": "^2.2.6",
    "clipboard": "^2.0.6",
    "docsearch.js": "^2.6.3",
    "eta": "^1.5.0",
    "infima": "0.2.0-alpha.11",
    "parse-numeric-range": "^1.2.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-player": "^2.6.1",
    "react-router-dom": "^5.2.0",
    "react-toggle": "^4.1.1",
    "theme-ui": "0.3.1",
    "use-media": "1.4.0"
  },
  "devDependencies": {
    "alex": "^9.0.1",
    "husky": "^4.2.3",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0"
  },
  "resolutions": {
    "babel-plugin-dynamic-import-node": "2.3.0"
  },
  "engines": {
    "node": ">=12.16.1"
  }
}
